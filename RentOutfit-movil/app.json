{
  "expo": {
    "name": "RO-RentOutfit",
    "slug": "ro",
    "scheme": "myapp",
    "host": "checkout",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.RORentOutfit",
      "associatedDomains": [
        "applinks:myapp.com"
      ]
    },
    "experiments": {
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.RORentOutfit",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "myapp",
              "host": "checkout-success"
            },
            {
              "scheme": "myapp",
              "host": "checkout-failure"
            },
            {
              "scheme": "myapp",
              "host": "checkout-pending"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "bac00fe9-e5e5-46af-84c9-be02d4bf2a1a"
      }
    },
    "owner": "ceron-eng2"
  }
}
